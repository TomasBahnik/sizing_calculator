    {
      "collapsed": false,
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "id": 2,
      "repeat": "environment",
      "title": "Overview - ${environment}",
      "type": "row"
    },
    {
      "datasource": {
        "uid": "$datasource"
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "continuous-GrYlRd"
          },
          "max": 1,
          "min": 0,
          "unit": "percentunit"
        }
      },
      "gridPos": {
        "h": 3,
        "w": 12,
        "x": 0,
        "y": 1
      },
      "id": 3,
      "interval": "2m",
      "options": {
        "displayMode": "lcd",
        "minVizHeight": false,
        "orientation": "horizontal",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "values": false
        },
        "showUnfilled": false
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${datasource}"
          },
          "editorMode": "code",
          "expr": "sum(rate(container_cpu_usage_seconds_total{namespace=~\"${namespace}\", image!=\"\", deploymentType=~\"${deploymentType}\", environment=~\"${environment}\"}[$__rate_interval])) / sum(machine_cpu_cores{deploymentType=~\"${deploymentType}\", environment=~\"${environment}\"})",
          "format": "time_series",
          "legendFormat": "Requests",
          "refId": "A"
        }
      ],
      "title": "Namespace(s) usage on total cluster CPU in %",
      "transparent": true,
      "type": "bargauge"
    },
    {
      "datasource": {
        "uid": "$datasource"
